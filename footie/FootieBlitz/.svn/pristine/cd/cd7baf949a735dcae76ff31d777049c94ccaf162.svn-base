package model;

public class PlayerTrainingInfo {
	
	public int personId;
	public int trainingFacilities;
	public double basicPP;
	public double totalPPFromXP;
	public double totalPPFromFacc;
	public Player player;
	
	public PlayerTrainingInfo(int personId, int trainingFacilities, double basicPP, double totalPPFromXP, double totalPPFromFacc, Player player) {
		super();
		this.personId = personId;
		this.trainingFacilities = trainingFacilities;
		this.basicPP = basicPP;
		this.totalPPFromXP = totalPPFromXP;
		this.totalPPFromFacc = totalPPFromFacc;
		this.player = player;
	}
	
	
}
